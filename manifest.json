{
  "manifest_version": 2,

  "name": "Astro Empires",
  "description": "Astro Empires.",
  "version": "1.0",

  "permissions": [
    "https://*/",
    "http://*/",
    "tabs",
    "notifications"
  ],

  "options_page": "options/options.html",

  "browser_action": {
    "default_icon": {
      "19": "images/icon.png"
    },
    "default_title": "Astro Empires",
    "default_popup": "popup.html"
  },

  "background": {
    "page": "background.html"
  }

}